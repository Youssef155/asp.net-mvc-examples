<div class="list-container">
    <h3>Cities</h3>
    <ul class="list">
        <li>London</li>
        <li>Cairo</li>
        <li>Alexandria</li>
        <li>Paris</li>
        <li>Tokyo</li>
    </ul>
</div>

<div class="list-container">
    <h3>@ViewBag.ListTitle</h3>
    <ul class="list">
        @foreach (var item in ViewBag.ListItems)
        {
            <li>@item</li>
        }

    </ul>
</div>